<% include partials/header %>

<div class="container">
  <header class="jumbotron">
    <div class="container">
    <h1>Camp Grounds</h1>
    <p>View our camp ground from all over India</p>
    <p>
      <a class="btn btn-primary btn-large" href="/campgrounds/new">Add new Campgrounds</a>
    </p>
  </div>
  </header>


<div class="row text-center" style="display:flex; flex-wrap:wrap;">
    <%campgrounds.forEach(function(camps){  %>
      <div class="col-md-3 col-sm-6">
        <div class="img-thumbnail">
            <img class="img-thumbnail" src="<%= camps.image  %>">
            <div class="caption">
              <h4><%= camps.name  %></h4>
            </div>
        </div>       
      </div>
    <% }); %> 
  </div>
</div>



<% include partials/footer %>